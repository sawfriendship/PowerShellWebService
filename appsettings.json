{
  "IsDevelopment": true,
  "ExecutionPolicy": "Unrestricted",
  "ScriptRoot": ".\\.scripts",
  "PwShUrl": "api",
  "Roles": {
    "Admin": ["BUILTIN\\Администраторы"],
    "User": ["BUILTIN\\Пользователи"]
  },
  "WrapperPermissions": {
    "prod": ["BUILTIN\\Администраторы","BUILTIN\\Пользователи"],
    "test": ["BUILTIN\\Администраторы"]
  },
  "JsonSerialization": {
    "compressOutput": false,
    "enumsAsStrings": true,
    "maxDepth": 4,
    "Fields": {
      "Error": true,
      "Warning": true,
      "Information": false,
      "Verbose": false
    }
  },
  "PSModulePath": [
    "C:\\Program Files\\PowerShell\\Modules",
    "c:\\program files\\powershell\\7\\Modules",
    "C:\\Program Files\\WindowsPowerShell\\Modules",
    "C:\\WINDOWS\\system32\\WindowsPowerShell\\v1.0\\Modules"
  ],
  "Variables": {
    "ErrorActionPreference": "Continue",
    "WarningPreference": "Continue",
    "VerbosePreference": "SilentlyContinue",
    "DebugPreference": "Continue",
    "ProgressPreference": "SilentlyContinue",
    "ErrorView": "NormalView",
    "FormatEnumerationLimit": 10,
    "OFS": ",",
    "TestVariable": 42
  },
  "UserCredentialVariable": "UserCredential",
  "CachedVariables": [
    "Credential",
    "StartUp"
  ],
  "SqlLogging": {
    "Enabled": false,
    "AbortScriptOnFailure": false,
    "Table": "Log",
    "Fields": {
      "Headers": true,
      "Query": true,
      "Body": true,
      "PSObjects": true,
      "StreamError": true,
      "StreamWarning": true,
      "StreamInformation": true,
      "StreamVerbose": true
    },
    "ConnectionString": "Data Source=localhost;Initial Catalog=PowerShellWebService;Integrated Security=true;Connection Timeout=5;"
  },
  "DateTimeLogFormat": "yyyy-MM-dd HH:mm:ss",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DetailedErrors": false
}

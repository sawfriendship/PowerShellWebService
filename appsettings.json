{
  "DetailedErrors": false,
  "AllowedHosts": "*",
  "Test": "222222222",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ExecutionPolicy": "Unrestricted",
  "DateTimeLogFormat": "yyyy-MM-dd HH:mm:ss",
  "Depth": 4,
  "ScriptRoot": ".\\.scripts",
  "UserCredentialVariable": "UserCredential",
  "Variables": {
    "ErrorActionPreference": "Continue",
    "WarningPreference": "Continue",
    "VerbosePreference": "SilentlyContinue",
    "DebugPreference": "Continue",
    "ProgressPreference": "SilentlyContinue",
    "ErrorView": "NormalView",
    "FormatEnumerationLimit": 10,
    "OFS": ",",
    "TestVariable": 42
  },
  "Roles": {
    "Admin": ["NET\\nest_test", "NET\\WorkstationAdmins"],
    "User": ["NET\\PwShApi"]
  },
  "Permission": {
    "prod": ["NET\\nest_test", "NET\\WorkstationAdmins"],
    "test": ["NET\\nest_test", "NET\\WorkstationAdmins"]
  },
  "CachedVariables": [
    "Credential",
    "StartUp"
  ],
  "SqlLogging": {
    "Enabled": true,
    "AbortScriptOnFailure": true,
    "Table": "Log",
    "ConnectionString": "Driver={ODBC Driver 18 for SQL Server};Server=dev;Database=PwSh;Authentication=ActiveDirectoryIntegrated;Encrypt=no;Connection Timeout=5;"
  }
}

{
	"IsDevelopment": true,
	"ExecutionPolicy": "Unrestricted",
	"ScriptRoot": ".\\.scripts",
	"TranscriptPath": ".\\.transcript",
	"PwShUrl": "api",
	"Roles": {
		"Admin": ["BUILTIN\\Администраторы"],
		"Operator": ["BUILTIN\\Администраторы"],
		"User": ["BUILTIN\\Пользователи"]
	},
	"WrapperPermissions": {
		"public": [],
		".example": ["BUILTIN\\Администраторы","BUILTIN\\Пользователи"]
	},
	"JsonSerialization": {
		"compressOutput": false,
		"enumsAsStrings": true,
		"maxDepth": 4,
		"Fields": {
			"Error": true,
			"Warning": true,
			"Information": false,
			"Verbose": false
		}
	},
	"PSModulePath": [
		"C:\\Program Files\\PowerShell\\Modules",
		"c:\\program files\\powershell\\7\\Modules",
		"C:\\Program Files\\WindowsPowerShell\\Modules",
		"C:\\WINDOWS\\system32\\WindowsPowerShell\\v1.0\\Modules"
	],
	"Variables": {
		"ErrorActionPreference": "Continue",
		"WarningPreference": "Continue",
		"VerbosePreference": "SilentlyContinue",
		"DebugPreference": "Continue",
		"ProgressPreference": "SilentlyContinue",
		"ErrorView": "NormalView",
		"FormatEnumerationLimit": 10,
		"OFS": ",",
		"TestVariable": 42
	},
	"UserCredentialVariable": "UserCredential",
	"CachedVariables": [
		"Credential",
		"StartUp"
	],
	"Always200": false,
	"SqlLogging": {
		"Enabled": true,
		"AbortScriptOnFailure": true,
		"Table": "Log",
		"ConnectionString": "Server=localhost;Database=PowerShellWebService;Connection Timeout=5;Integrated Security=false;User=;Password=",
		"Fields": {
			"Headers": true,
			"Query": true,
			"Body": true,
			"PSObjects": true,
			"StreamError": true,
			"StreamWarning": true,
			"StreamInformation": true,
			"StreamVerbose": true
		}
	},
	"DateTimeLogFormat": "yyyy-MM-dd HH:mm:ss",
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"DetailedErrors": false
}